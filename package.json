{
  "name": "hydro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "ios": "react-native run-ios --simulator 'IPhone 8'",
    "android": "react-native run-android",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "tsc": "tsc --noemit",
    "tsc:watch": "tsc --noemit --watch",
    "tslint": "tslint --project . -t stylish",
    "tslint:watch": "nodemon -e ts,tsx -w src -w tslint.json -d 0.2 -w tsconfig.json -- node_modules/.bin/tslint --project . -t stylish",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-community/slider": "^2.0.8",
    "appcenter": "^2.6.0",
    "appcenter-analytics": "^2.6.1",
    "appcenter-crashes": "^2.6.1",
    "bugsnag-react-native": "^2.23.5",
    "es6-symbol": "^3.1.3",
    "jsc-android": "241213.1.0",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.5",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-animatable": "^1.3.3",
    "react-native-collapsible": "^1.5.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-safe-area-view": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@react-native-community/cli": "^3.0.4",
    "@types/enzyme": "^3.10.4",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.30",
    "@types/requirejs": "^2.1.31",
    "@welldone-software/why-did-you-render": "^3.5.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-react-native": "^4.0.1",
    "babel-runtime": "^6.26.0",
    "datauri": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^4.2.0",
    "jest": "24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-sonar-reporter": "^2.0.0",
    "jetifier": "^1.6.5",
    "jsdom": "^16.0.1",
    "lint-staged": "^9.5.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "nodemon": "^2.0.2",
    "react-dom": "^16.12.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.12.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.2",
    "remotedev": "^0.2.9",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.1.0",
    "tslint-react-native": "^0.0.7",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn tslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run tsc && yarn test"
    }
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|react-navigation)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>jest-setup.js"
    ],
    "globals": {
      "window": {}
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": ".*\\.test.tsx?",
    "testEnvironment": "jsdom"
  },
  "volta": {
    "node": "10.16.3",
    "yarn": "1.17.3"
  }
}
